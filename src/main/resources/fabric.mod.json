{
  "schemaVersion": 1,
  "id": "netherstarevolution",
  "version": "${version}",
  "name": "Nether Star Evolution",
  "description": "A Fabric mod that adds more uses for nether stars by adding a new tier of equipment to the game.",
  "authors": [
    "SGDaGangsta"
  ],
  "contact": {
    "homepage": "https://example.com/",
    "issues": "https://github.com/SGDaGangsta/nether-star-evolution/issues",
    "sources": "https://github.com/SGDaGangsta/nether-star-evolution"
  },
  "license": "GPLv3",
  "icon": "assets/netherstarevolution/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "net.sgdagangsta.netherstarevolution.NetherStarEvolution"
      }
    ],
    "client": [
      {
        "adapter": "kotlin",
        "value": "net.sgdagangsta.netherstarevolution.NetherStarEvolutionClient"
      }
    ]
  },
  "mixins": [
    "netherstarevolution.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14.9",
    "fabric-api": "*",
    "fabric-language-kotlin": ">=1.8.5+kotlin.1.7.20",
    "minecraft": "~1.19",
    "java": ">=17"
  }
}